        rdfs:label                 "Élevage du bétail (y compris l'apiculture et la reproduction des animaux de compagnie)"@fr-CA ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-fra row: 64" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS108 ;
